{
  "name": "unbird",
  "version": "0.1.1",
  "main": "dist/main.js",
  "author": "Unbird",
  "license": "MIT",
  "scripts": {
    "test": "cross-env NODE_ENV=test yarn scripts:locale && jest",
    "test:coverage": "yarn test --coverage",
    "test:watch": "yarn test --watch",
    "test:debug": "node --inspect-brk jest --runInBand --watch",
    "test:e2e:run": "npm-run-all --parallel start cy:run",
    "test:e2e:open": "npm-run-all --parallel start cy:open",
    "tsc": "node_modules/.bin/tsc --noEmit --watch",
    "react-static:build": "node_modules/.bin/parcel build src/app/view/*.html  --public-url ./",
    "react-start": "npm-run-all --parallel  react-start:server scripts:locale:watch",
    "react-start:server": "node_modules/.bin/parcel server -p 3000 src/app/view/index.html",
    "clean-build": "rm -rf build/ .cache dist/ || true",
    "lint": "sh ./scripts/bash/lint.sh",
    "prettier:format": "node_modules/.bin/prettier --write src/**/*.ts{,x}",
    "prettier:check": "node_modules/.bin/prettier --check src/**/*.ts{,x}",
    "storybook": "start-storybook -p 6006 --no-dll",
    "build-storybook": "build-storybook --no-dll",
    "generate": "graphql-codegen && prettier --write ./src/lib/graphql/types.ts && eslint ./src/lib/graphql/types.ts --fix --quiet",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "scripts": "ts-node --project scripts/tsconfig.json",
    "scripts:locale": "mkdir -pv dist/locales || true && yarn scripts ./scripts/locales.ts",
    "scripts:locale:watch": "node_modules/.bin/nodemon --exec yarn scripts:locale --watch ./src/**/locales/*.json",
    "checkTypes": "node_modules/.bin/tsc --noEmit",
    "build": "sh ./scripts/bash/build.sh",
    "publish:app": "sh ./scripts/bash/publish.sh",
    "start": "sh ./scripts/bash/start.sh"
  },
  "devDependencies": {
    "@apollo/client": "^3.3.7",
    "@babel/core": "^7.10.1",
    "@babel/node": "^7.12.6",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.7",
    "@babel/preset-typescript": "^7.12.7",
    "@graphql-codegen/cli": "^1.19.2",
    "@graphql-codegen/typescript": "^1.17.11",
    "@graphql-codegen/typescript-apollo-client-helpers": "^1.1.0",
    "@graphql-codegen/typescript-operations": "^1.17.10",
    "@graphql-codegen/typescript-react-apollo": "^2.1.1",
    "@optimizely/react-sdk": "^2.4.1",
    "@storybook/addon-actions": "^6.0.28",
    "@storybook/addon-essentials": "^6.0.28",
    "@storybook/addon-links": "^6.0.28",
    "@storybook/react": "^6.0.28",
    "@testing-library/cypress": "^7.0.2",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@testing-library/user-event": "^12.6.0",
    "@types/async": "^3.2.5",
    "@types/dom-mediacapture-record": "^1.0.7",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.7",
    "@types/react": "^16.9.56",
    "@types/react-dom": "^16.9.9",
    "@types/react-router-dom": "^5.1.7",
    "@types/styled-components": "^5.1.4",
    "@types/styled-system": "^5.1.10",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.1",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-styled-components": "^1.11.1",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.2",
    "cypress": "^6.0.1",
    "eslint": "7.2.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-react-app": "^4.0.1",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "4.0.0",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "msw": "^0.24.2",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "parcel": "^1.12.4",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-custom-dist-structure": "^1.1.16",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.1.0",
    "react-app-polyfill": "^2.0.0",
    "react-content-loader": "^5.1.4",
    "react-is": "^17.0.1",
    "react-test-renderer": "^17.0.1",
    "reflect-metadata": "^0.1.13",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.0.0",
    "typescript": "^4.2.3",
    "typescript-plugin-styled-components": "^1.4.4",
    "wait-on": "^5.2.0"
  },
  "displayName": "Unbird",
  "dependencies": {
    "@hookform/resolvers": "^1.3.5",
    "async": "^3.2.0",
    "axios": "^0.21.1",
    "components": "^0.1.0",
    "domain": "^0.0.1",
    "dotenv": "^8.2.0",
    "framer-motion": "^2.9.4",
    "graphql": "^15.4.0",
    "graphql-tag": "^2.11.0",
    "hls.js": "^0.14.17",
    "i18next": "^19.8.4",
    "i18next-browser-languagedetector": "^6.0.1",
    "i18next-http-backend": "^1.0.21",
    "jwt-decode": "^3.1.2",
    "keyboardjs": "^2.6.4",
    "lib": "^4.3.0",
    "moment": "^2.29.1",
    "mousetrap": "^1.6.5",
    "mousetrap-global-bind": "^1.1.0",
    "nanoid": "^3.1.16",
    "nock": "^13.0.5",
    "pusher-js": "^7.0.1",
    "react": "^17.0.1",
    "react-circular-progressbar": "^2.0.3",
    "react-dom": "^17.0.1",
    "react-hook-form": "^6.15.5",
    "react-i18next": "^11.7.3",
    "react-laag": "^1.8.0",
    "react-router-dom": "^5.2.0",
    "react-segment": "^0.1.8",
    "react-toastify": "^7.0.1",
    "react-use": "^15.3.4",
    "react-virtualized": "^9.22.2",
    "resize-observer-polyfill": "^1.5.1",
    "socket.io-client": "2.3.1",
    "sparkpost": "^2.1.4",
    "styled-components": "^5.2.1",
    "uuid": "^8.3.2",
    "yup": "^0.32.9"
  },
  "browserslist": [
    "last 1 Chrome version"
  ],
  "husky": {
    "pre-commit": "node_modules/.bin/tsc --noEmit && pretty-quick --staged",
    "pre-push": "sh ./scripts/bash/lint.sh"
  },
  "eslintConfig": {
    "globals": {
      "log": true
    }
  },
  "customDistStructure": {
    "config": {
      ".js": "./",
      ".js.map": "./",
      ".html": "./"
    },
    "options": {
      "development": true
    }
  }
}
